  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development tsnd --respawn --transpile-only src/server.ts",
    "dockerStartDev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "dockerEndDev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down -v",
    "dockerStartProd": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d --build",
    "dockerEndProd": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml down -v",
    "logs": "docker logs talk-backend_talk-app_1 -f",
    "build": "rm -rf dist && tsc"
  },
